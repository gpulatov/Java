
public class Dog extends Animal {
	
	private int eyes;
	private int legs;
	private int tail;
	private int teeth;
	private String coat;
	public Dog(String name, int brain, int body, int weight, int eyes, int legs, int tail, int teeth, String coat) {
		super(name, brain, body, weight);
		this.legs = legs;
		this.tail = tail;
		this.teeth = teeth;
		this.coat = coat;
		this.eyes = eyes;
		
	}
	
	
	

}
